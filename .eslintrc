{
  "parser": "babel-eslint",
  "extends": ["airbnb-base", "plugin:jest/recommended"],
  "plugins": ["jest"],
  "env": {
    "jest/globals": true
  },
  "rules": {
    # "strict": 0,
    "quotes": [
        "error",
        "double"
    ],
    "semi": [
        "error",
        "never"
    ],
    "no-underscore-dangle": [
      "error",
      { "allow": ["_id"] }
    ]
  }
}
